{
  "schema": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "gumball",
    "class": "baseADO",
    "classifier": "sale",
    "title": "Gumball",
    "description": "Allows users to buy an NFT randomly from a collection.",

    "type": "object",
    "required": ["andromeda_cw721_contract", "randomness_source"],
    "properties": {
      "andromeda_cw721_contract": {
        "type": "object",
        "required": ["identifier"],
        "properties": {
          "identifier": {
            "description": "Can be either an address or identifier of an ADO in a app.",
            "type": "string"
          }
        },
        "title": "andromeda_cw721_contract"
      },
      "randomness_source": { "type": "string", "title": "randomness_source" }
    }
  },
  "ui-schema": {},
  "form-data": {}
}

{
  "schema": {
    "title": "Update Pricing",
    "description": "Allows an ADO owner to update or remove the listing price of their ADO. Token # 3787sd78543",
    "$id": "/schemas/ado-modifier/update-pricing",
    "type": "object",
    "required": ["token_id"],
    "properties": {
      "token_id": {
        "type": "string",
        "title": "Token Id"
      },
      "boolean": {
        "type": "object",
        "title": "Price",
        "properties": {
          "amount": {
            "type": "string",
            "title": "Amount"
          },
          "denom": {
            "type": "string",
            "title": "Currency Type",
            "enum": ["ust", "luna"],
            "enumNames": ["UST", "LUNA"],
            "default": "ust",
            "ui:group": true
          }
        }
      }
    }
  },
  "ui-schema": {
    "amount": {
      "ui:help": "description of price amount",
      "denom": { "ui:help": "description of price currency type" }
    }
  },
  "form-data": {}
}

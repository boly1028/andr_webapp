{
  "schema": {
    "title": "Update Strategy",
    "description": "Updates the contract address used for the specified strategy.",
    "type": "object",
    "required": ["update_strategy"],
    "properties": {
      "update_strategy": {
        "type": "object",
        "required": ["address", "strategy"],
        "properties": {
          "address": {
            "type": "object",
            "required": ["identifier"],
            "properties": {
              "identifier": {
                "description": "Can be either an address or identifier of an ADO in a mission.",
                "type": "string"
              }
            },
            "title": "address"
          },
          "strategy": {
            "type": "string",
            "enum": ["anchor"],
            "title": "strategy"
          }
        }
      }
    },
    "additionalProperties": false
  },
  "ui-schema": {},
  "form-data": {}
}

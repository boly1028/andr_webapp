{
    "schema": {
        "$id": "embeddables-app",
        "$schema": "http://json-schema.org/draft-07/schema#",
        "class": "system",
        "classifier": "",
        "title": "Embeddable",
        "description": "Publishing configurations to publish the ADO as an App",
        "type": "object",
        "required": [
            "chainId",
            "coinDenom",
            "name"
        ],
        "properties": {
            "chainId": {
                "type": "string",
                "title": "Chain Id",
                "description": "Chain Id for the app"
            },
            "coinDenom": {
                "type": "string",
                "title": "Coin Denom",
                "description": "Sale Coin Denomination"
            },
            "name": {
                "type": "string",
                "title": "App Name",
                "description": "The desired name of the app."
            },
            "twitter": {
                "type": "string",
                "title": "Twitter URL",
                "description": "Twitter url for the embeddable"
            },
            "banner": {
                "type": "string",
                "title": "Banner Image URL",
                "description": "The desired name of the app."
            }
        }
    },
    "ui-schema": {},
    "form-data": {}
}
{
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Address list",
        "type": "object",
        "required": [
            "is_inclusive",
            "kernel_address"
        ],
        "properties": {
            "is_inclusive": {
                "type": "boolean",
                "title": "Is inclusive"
            },
            "kernel_address": {
                "type": "string",
                "title": "Kernel address",
                "default": ""
            },
            "owner": {
                "type": [
                    "string",
                    "null"
                ],
                "title": "Owner"
            }
        },
        "additionalProperties": false,
        "$id": "address-list",
        "version": "0.2.1",
        "classifier": "address",
        "class": "module",
        "description": "ADO that is used to store a list of addresses."
    },
    "ui-schema": {
        "is_inclusive": {
            "ui:options": {
                "title": "Is Inclusive",
                "description": "Whether or not the address list is inclusive.",
                "help": "If true the address list is a whitelist. If false the address list is a blacklist."
            }
        },
        "kernel_address": {
            "ui:widget": "hidden"
        }
    },
    "form-data": {}
}
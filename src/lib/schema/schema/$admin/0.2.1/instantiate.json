{
    "schema": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Instantiate",
        "type": "object",
        "required": [
            "msg",
            "codeId"
        ],
        "properties": {
            "msg": {
                "title": "Message",
                "description": "JSON which will be sent as instantiate Message",
                "type": "string",
                "$original_type": "JSON"
            },
            "label": {
                "title": "Label",
                "description": "Label for instantiate Message",
                "type": "string"
            },
            "codeId": {
                "title": "Code Id",
                "description": "Code Id used to instantiate new ado. Use Ado Type unless you know what you are doing",
                "oneOf": [
                    {
                        "title": "Code Id",
                        "description": "Code ID of the uploaded contract you want to use to instantiate the ado",
                        "type": "number"
                    },
                    {
                        "title": "Ado Type",
                        "description": "Use ado type key added in our adodb for easy access. No need to remember code id",
                        "type": "string"
                    }
                ]
            }
        },
        "additionalProperties": false,
        "$id": "instantiate",
        "version": "0.2.1",
        "classifier": "system",
        "class": "system",
        "description": "Instantiate Ado"
    },
    "ui-schema": {
        "msg": {
            "ui:widget": "instantiateMsg"
        }
    },
    "form-data": {}
}